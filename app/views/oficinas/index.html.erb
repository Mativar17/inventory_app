<p id="notice"><%= notice %></p>

<h1>Oficinas</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sku</th>
      <th>Valor neto unitario</th>
      <th>Stock inicial</th>
      <th>Compra mes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @oficinas.each do |oficina| %>
      <tr>
        <td><%= oficina.name %></td>
        <td><%= oficina.sku %></td>
        <td><%= oficina.valor_neto_unitario %></td>
        <td><%= oficina.stock_inicial %></td>
        <td><%= oficina.compra_mes %></td>
        <td><%= link_to 'Show', oficina %></td>
        <td><%= link_to 'Edit', edit_oficina_path(oficina) %></td>
        <td><%= link_to 'Destroy', oficina, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Oficina', new_oficina_path %>
